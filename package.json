{
  "name": "optionsmarketliquiditypool",
  "version": "1.0.0",
  "description": "A liquidity pool that provides dynamic underlying assets for option contract market orders on-demand and distributes premiums to pool members",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "truffle build",
    "compile": "truffle compile",
    "migrate": "truffle migrate",
    "test": "truffle test --stacktrace",
    "test_debug": "truffle test --debug",
    "coverage": "truffle run coverage",
    "solhint": "./node_modules/.bin/solhint -f table contracts/*.sol contracts/**/*.sol",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/*.sol contracts/**/*.sol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YieldHand/OptionsMarketLiquidityPool.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YieldHand/OptionsMarketLiquidityPool/issues"
  },
  "homepage": "https://github.com/YieldHand/OptionsMarketLiquidityPool#readme",
  "dependencies": {
    "truffle": "^5.4.2"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.2.0",
    "eth-gas-reporter": "^0.2.22",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.16"
  }
}
